{
  "name": "Blocky Borders Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#1E1E1E",
    "editorLineNumber.foreground": "#AAAAAA",
    "editorLineNumber.activeForeground": "#000000",
    "editor.lineHighlightBackground": "#F3F3F3",
    "editor.selectionBackground": "#C0C0C0",
    "editor.selectionHighlightBackground": "#00000010",
    "editorIndentGuide.background": "#E0E0E0",
    "editorIndentGuide.activeBackground": "#999999",
    "editorCursor.foreground": "#FF8800",
    "editorGroup.border": "#CCCCCC",
    "editorGroupHeader.tabsBorder": "#CCCCCC",
    "tab.border": "#CCCCCC",
    "tab.activeBackground": "#FFFFFF",
    "tab.inactiveBackground": "#F5F5F5",
    "tab.activeForeground": "#1E1E1E",
    "tab.inactiveForeground": "#555555",
    "sideBar.background": "#F5F5F5",
    "sideBar.foreground": "#1E1E1E",
    "sideBar.border": "#CCCCCC",
    "sideBarTitle.foreground": "#000000",
    "sideBarSectionHeader.background": "#F0F0F0",
    "sideBarSectionHeader.foreground": "#1E1E1E",
    "activityBar.background": "#F0F0F0",
    "activityBar.foreground": "#1E1E1E",
    "activityBar.border": "#CCCCCC",
    "activityBarBadge.background": "#FFCC00",
    "activityBarBadge.foreground": "#FFFFFF",
    "statusBar.background": "#F0F0F0",
    "statusBar.foreground": "#1E1E1E",
    "statusBar.border": "#CCCCCC",
    "statusBar.debuggingBackground": "#E0E0E0",
    "statusBar.debuggingForeground": "#FF8800",
    "panel.background": "#F5F5F5",
    "panel.border": "#CCCCCC",
    "panelTitle.activeForeground": "#1E1E1E",
    "panelTitle.inactiveForeground": "#555555",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#1E1E1E",
    "terminalCursor.foreground": "#FF8800",
    "terminal.selectionBackground": "#C0C0C0",
    // Buttons
    "button.background": "#E0E0E0",
    "button.foreground": "#1E1E1E",
    "button.hoverBackground": "#D0D0D0",
    "button.secondaryBackground": "#F0F0F0",
    "button.secondaryForeground": "#1E1E1E",
    "button.secondaryHoverBackground": "#D8D8D8",
    "button.border": "#CCCCCC",
    // Quick pick / dropdown buttons
    "dropdown.background": "#F5F5F5",
    "dropdown.foreground": "#1E1E1E",
    "dropdown.border": "#CCCCCC",
    // Input box buttons
    "input.background": "#F5F5F5",
    "input.foreground": "#1E1E1E",
    "input.border": "#CCCCCC",
    "inputOption.activeBorder": "#FF8800"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#228B22",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#A52A2A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#2E8B57"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#6A5ACD"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#E06666",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "support.function",
        "support.constant",
        "support.type"
      ],
      "settings": {
        "foreground": "#1E90FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation",
        "operator"
      ],
      "settings": {
        "foreground": "#1E1E1E"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": [
        "deprecated"
      ],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#B8860B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#B8860B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#5B8E7E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.list",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#D2691E"
      }
    },
    {
      "scope": [
        "meta",
        "meta.structure"
      ],
      "settings": {
        "foreground": "#1E1E1E"
      }
    },
    {
      "scope": [
        "keyword.operator.unpacking",
        "punctuation.definition.operator"
      ],
      "settings": {
        "foreground": "#FF8800"
      }
    }
  ]
}